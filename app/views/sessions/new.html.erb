<h1 class="text-danger">Login</h1>
<%= form_for(:session, local: true, url: login_path) do |form| %>
<form>
  <div class="form-group">
    <label for="email">
      <%= form.label :email %>
     </label>
    <%= form.email_field :email, class: "form-control", placeholder: "Enter Email" %>
  </div>

   <div class="form-group">
    <label for="title">
      <%= form.label :password %>
    </label>
    <%= form.password_field :password, class: "form-control", placeholder: "Enter Password", autocomplete: 'off' %>
  </div>

  <br>
  <div class="form-group">
    <%= form.submit 'Login', class: "btn btn-danger" %>
  </div>
<% end %>
</form>
